  <div><h2  style= "color:white; text-align: center;font-family:Arial, Helvetica, sans-serif;margin-top:4vh">Display User Details</h2></div>
 <div >
<table class = "table table-striped"  width='50' height = "50" border="1">
  <thead class="thead-dark"> <tr>
           <th scope="col">Name</th>
            <th scope="col">Email Id</th>
            <th scope="col">Address</th>
            <th scope="col">Contact No</th>
         </tr></thead>
         <% userdata.forEach(function(user){ %>
            <tr>
               <td  scope="row"><%= user.name %></td>
               <td  scope="row"><%= user.email %></td>
               <td  scope="row"><%= user.address %></td>
               <td  scope="row"><%= user.phone_no %></td>
            </tr>
         <% } )%>
           
         
    </table>
 
